{
    "name": "lambda9/integration_signd",
    "description": "Digital document signing with signd.it for Nextcloud",
    "type": "project",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "lambda9 GmbH",
            "email": "support@lambda9.de"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "nextcloud/ocp": "^30.0",
        "guzzlehttp/guzzle": "^7.10"
    },
    "autoload": {
        "psr-4": {
            "OCA\\IntegrationSignd\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OCA\\IntegrationSignd\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
