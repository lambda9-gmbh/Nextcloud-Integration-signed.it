{
    "name": "integration_signd",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "watch": "vite build --watch",
        "build": "vite build",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "occ": "docker compose exec -u www-data nextcloud php occ",
        "enable-app": "docker compose exec -u www-data nextcloud php occ app:enable integration_signd",
        "up": "docker compose up -d",
        "down": "docker compose down",
        "restart": "docker compose down && docker compose up -d",
        "logs": "docker compose logs -f nextcloud"
    },
    "dependencies": {
        "@nextcloud/axios": "^2.5.0",
        "@nextcloud/files": "^3.9.0",
        "@nextcloud/initial-state": "^2.2.0",
        "@nextcloud/l10n": "^3.1.0",
        "@nextcloud/router": "^3.0.1",
        "@nextcloud/vue": "^9.5.0",
        "vue": "^3.4.0"
    },
    "devDependencies": {
        "@nextcloud/vite-config": "^2.3.0",
        "@types/node": "^20.0.0",
        "sass": "^1.77.0",
        "typescript": "^5.5.0",
        "vite": "^7.1.10"
    },
    "engines": {
        "node": "^20.0.0",
        "npm": "^10.0.0"
    }
}
